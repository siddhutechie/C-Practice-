#include <iostream>
using namespace std;

int main() {
    //Variable
    int number;

    //Variable input
    cin >> number;

    //Countdown process
    while (number > 0) {
        if (number % 5 == 0) {
            cout << "\n" << number << "\n" << "Beep";
            number--;
        }
        cout << "\n" << number;
        number--;
    }
}
